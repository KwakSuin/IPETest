                android:layout_height="wrap_content"
                android:text="오답"
                android:textColor="#ff0000"
                android:layout_marginRight="10dp"
                android:visibility="invisible"/>
            <Button
                android:id="@+id/hint47_bt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
