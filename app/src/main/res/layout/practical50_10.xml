<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".practical.problem50.Practical50_10">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/text46"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/practical50_046"
            android:textColor="#000000"
            android:layout_marginTop="10dp"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="right">
            <TextView
                android:id="@+id/hint46"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TCP"
                android:textColor="#000000"
                android:visibility="invisible"
                android:layout_marginRight="10dp"/>
            <TextView
                android:id="@+id/error46"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="오답"
                android:textColor="#ff0000"
                android:layout_marginRight="10dp"
                android:visibility="invisible"/>
            <Button
                android:id="@+id/hint46_bt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="정답"
                android:backgroundTint="#BDBDBD"
                android:layout_marginRight="10dp"/>
        </LinearLayout>
        <EditText
            android:id="@+id/answer46"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="20"
            android:hint="정답"
            android:textColor="#005766" />

