<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#FFF2E6"
    tools:context=".practical.exam.PracticalExam_menu">

    <TextView
        android:id="@+id/exam01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2020년 제 1회 실기"
        android:layout_marginTop="130dp"
        android:textColor="#000000"
        android:layout_gravity="center"
        android:textSize="30sp"
        />

    <TextView
        android:id="@+id/exam02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2020년 제 2회 실기"
        android:layout_marginTop="30dp"
        android:textColor="#000000"
        android:layout_gravity="center"
        android:textSize="30sp"
        />

    <TextView
        android:id="@+id/exam03"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2020년 제 3회 실기"
        android:layout_marginTop="30dp"
        android:textColor="#000000"
        android:layout_gravity="center"
        android:textSize="30sp"
        />

    <TextView
        android:id="@+id/exam04"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="2020년 제 4,5회 실기"
        android:layout_marginTop="30dp"
        android:textColor="#000000"
        android:layout_gravity="center"
        android:textSize="30sp"
        />

</LinearLayout>