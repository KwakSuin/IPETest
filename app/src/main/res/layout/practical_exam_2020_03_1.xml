<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".practical.exam.PracticalExam_2020_03_1">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="2020년 제 3회 실기문제(코드제외)"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:id="@+id/text01"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/PR_Exam2020_03_01"
            android:textColor="#000000"
            android:layout_marginTop="10dp"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="right">
            <Button
                android:id="@+id/hint01_bt"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/button_circle"
                android:text="정답"
                app:backgroundTint="#BDBDBD"/>
        </LinearLayout>
        <EditText
            android:id="@+id/answer01"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="50"
            android:hint="정답"
            android:textColor="#005766" />
        <TextView
            android:id="@+id/hint01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="소프트웨어를 보다 이해하기 쉽고, 수정하기 쉽도록 만드는 것"
            android:textColor="#000000"
            android:visibility="invisible"
            android:layout_marginRight="10dp"/>



        <TextView
            android:id="@+id/text02"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/PR_Exam2020_03_02"
            android:textColor="#000000"
            android:layout_marginTop="10dp"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="right">
            <Button
                android:id="@+id/hint02_bt"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_marginRight="10dp"
                android:background="@drawable/button_circle"
                android:text="정답"
                app:backgroundTint="#BDBDBD"/>
        </LinearLayout>
        <EditText
            android:id="@+id/answer02"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="50"
            android:hint="정답"
            android:textColor="#005766" />
        <TextView
            android:id="@+id/hint02"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="OSPF"
            android:textColor="#000000"
            android:visibility="invisible"
            android:layout_marginRight="10dp"/>



        <TextView
            android:id="@+id/text03"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/PR_Exam2020_03_03"
            android:textColor="#000000"
            android:layout_marginTop="10dp"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="right">
            <Button
                android:id="@+id/hint03_bt"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_marginRight="10dp"
