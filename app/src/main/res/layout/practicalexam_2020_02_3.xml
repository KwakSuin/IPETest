<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".practical.exam.PracticalExam_2020_02_3">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="2020년 제 2회 실기문제(코드제외)"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/text11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/PR_Exam2020_02_11"
            android:textColor="#000000"
            android:layout_marginTop="10dp"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="right">
            <Button
                android:id="@+id/hint11_bt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="정답"
                android:backgroundTint="#BDBDBD"
                android:layout_marginRight="10dp"/>
        </LinearLayout>
        <EditText
            android:id="@+id/answer11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="50"
            android:hint="정답"
            android:textColor="#005766" />
        <TextView
            android:id="@+id/hint11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="유효성"
            android:textColor="#000000"
            android:visibility="invisible"
            android:layout_marginRight="10dp"/>



        <TextView
            android:id="@+id/text12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/PR_Exam2020_02_12"
            android:textColor="#000000"
            android:layout_marginTop="10dp"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="right">
            <Button
                android:id="@+id/hint12_bt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="정답"
                android:backgroundTint="#BDBDBD"
                android:layout_marginRight="10dp"/>
        </LinearLayout>
        <EditText
            android:id="@+id/answer12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="50"
            android:hint="정답"
            android:textColor="#005766" />
        <TextView
            android:id="@+id/hint12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="chmod 751 a.txt"
            android:textColor="#000000"
            android:visibility="invisible"
            android:layout_marginRight="10dp"/>



        <TextView
            android:id="@+id/text13"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/PR_Exam2020_02_13"
            android:textColor="#000000"
            android:layout_marginTop="10dp"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="right">
            <Button
                android:id="@+id/hint13_bt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="정답"
                android:backgroundTint="#BDBDBD"
                android:layout_marginRight="10dp"/>
        </LinearLayout>
        <EditText
            android:id="@+id/answer13"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="50"
            android:hint="정답"
            android:textColor="#005766" />
        <TextView
            android:id="@+id/hint13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Linked Open Data"
            android:textColor="#000000"
            android:visibility="invisible"
            android:layout_marginRight="10dp"/>



        <TextView
            android:id="@+id/text14"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/PR_Exam2020_02_14"
            android:textColor="#000000"
            android:layout_marginTop="10dp"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="right">
            <Button
                android:id="@+id/hint14_bt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="정답"
                android:backgroundTint="#BDBDBD"
                android:layout_marginRight="10dp"/>
        </LinearLayout>
        <EditText
            android:id="@+id/answer14"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="50"
            android:hint="정답"
            android:textColor="#005766" />
        <TextView
            android:id="@+id/hint14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="개념적 설계, 논리적 설계, 물리적 설계"
            android:textColor="#000000"
            android:visibility="invisible"
            android:layout_marginRight="10dp"/>



        <TextView
            android:id="@+id/text15"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/PR_Exam2020_02_15"
            android:textColor="#000000"
            android:layout_marginTop="10dp"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="right">
            <Button
                android:id="@+id/hint15_bt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="정답"
                android:backgroundTint="#BDBDBD"
                android:layout_marginRight="10dp"/>
        </LinearLayout>
        <EditText
            android:id="@+id/answer15"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="50"
            android:hint="정답"
            android:textColor="#005766" />
        <TextView
            android:id="@+id/hint15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="형상관리"
            android:textColor="#000000"
            android:visibility="invisible"
            android:layout_marginRight="10dp"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <Button
            android:id="@+id/back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="이전"
            android:layout_marginLeft="10dp"
            android:backgroundTint="#BDBDBD"
            android:textColor="#000000"/>
        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1"/>

        <Button
            android:id="@+id/end"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="확인"
            android:layout_marginRight="10dp"
            android:backgroundTint="#BDBDBD"
            android:textColor="#000000"/>
    </LinearLayout>
</LinearLayout>