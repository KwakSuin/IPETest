<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".practical.exam.PracticalExam_2020_03_2">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="2020년 제 3회 실기문제(코드제외)"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:textColor="#000000"
        android:textSize="20sp"
        android:textStyle="bold"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:id="@+id/text07"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/PR_Exam2020_03_07"
            android:textColor="#000000"
            android:layout_marginTop="10dp"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="right">
            <Button
                android:id="@+id/hint07_bt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="정답"
                android:backgroundTint="#BDBDBD"
                android:layout_marginRight="10dp"/>
        </LinearLayout>
        <EditText
            android:id="@+id/answer07"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="50"
            android:hint="정답"
            android:textColor="#005766" />
        <TextView
            android:id="@+id/hint07"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="블랙박스 테스트(명세 기반 테스트)"
            android:textColor="#000000"
            android:visibility="invisible"
            android:layout_marginRight="10dp"/>



        <TextView
            android:id="@+id/text08"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/PR_Exam2020_03_08"
            android:textColor="#000000"
            android:layout_marginTop="10dp"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="right">
            <Button
                android:id="@+id/hint08_bt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="정답"
                android:backgroundTint="#BDBDBD"
                android:layout_marginRight="10dp"/>
        </LinearLayout>
        <EditText
            android:id="@+id/answer08"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="50"
            android:hint="정답"
            android:textColor="#005766" />
        <TextView
            android:id="@+id/hint08"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="데이터베이스의 구조와 제약조건에 관한 전반적인 명세를 기술한 메타데이터의 집합"
            android:textColor="#000000"
            android:visibility="invisible"
            android:layout_marginRight="10dp"/>



        <TextView
            android:id="@+id/text09"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/PR_Exam2020_03_09"
            android:textColor="#000000"
            android:layout_marginTop="10dp"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="right">
            <Button
                android:id="@+id/hint09_bt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="정답"
                android:backgroundTint="#BDBDBD"
                android:layout_marginRight="10dp"/>
        </LinearLayout>
        <EditText
            android:id="@+id/answer09"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="50"
            android:hint="정답"
            android:textColor="#005766" />
        <TextView
            android:id="@+id/hint09"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Point to Point, Hub n Spoke"
            android:textColor="#000000"
            android:visibility="invisible"
            android:layout_marginRight="10dp"/>



        <TextView
            android:id="@+id/text10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/PR_Exam2020_03_10"
            android:textColor="#000000"
            android:layout_marginTop="10dp"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="right">
            <Button
                android:id="@+id/hint10_bt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="정답"
                android:backgroundTint="#BDBDBD"
                android:layout_marginRight="10dp"/>
        </LinearLayout>
        <EditText
            android:id="@+id/answer10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="50"
            android:hint="정답"
            android:textColor="#005766" />
        <TextView
            android:id="@+id/hint10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="누구나 쉽게 이해하고, 쉽게 사용할 수 있어야 하고 쉬운 검색, 쉬운 사용성, 일관성의 부특성을 가지고 있는 UI 설계원칙"
            android:textColor="#000000"
            android:visibility="invisible"
            android:layout_marginRight="10dp"/>



        <TextView
            android:id="@+id/text11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/PR_Exam2020_03_11"
            android:textColor="#000000"
            android:layout_marginTop="10dp"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="right">
            <Button
                android:id="@+id/hint11_bt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="정답"
                android:backgroundTint="#BDBDBD"
                android:layout_marginRight="10dp"/>
        </LinearLayout>
        <EditText
            android:id="@+id/answer11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="50"
            android:hint="정답"
            android:textColor="#005766" />
        <TextView
            android:id="@+id/hint11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="객체 초기화 함수 객체 생성 시 자동호출되는 메소드"
            android:textColor="#000000"
            android:visibility="invisible"
            android:layout_marginRight="10dp"/>



        <TextView
            android:id="@+id/text12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/PR_Exam2020_03_12"
            android:textColor="#000000"
            android:layout_marginTop="10dp"/>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_gravity="right">
            <Button
                android:id="@+id/hint12_bt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="정답"
                android:backgroundTint="#BDBDBD"
                android:layout_marginRight="10dp"/>
        </LinearLayout>
        <EditText
            android:id="@+id/answer12"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:ems="50"
            android:hint="정답"
            android:textColor="#005766" />
        <TextView
            android:id="@+id/hint12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ALTER, ADD"
            android:textColor="#000000"
            android:visibility="invisible"
            android:layout_marginRight="10dp"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <Button
            android:id="@+id/back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="이전"
            android:layout_marginLeft="10dp"
            android:backgroundTint="#BDBDBD"
            android:textColor="#000000"/>
        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1"/>

        <Button
            android:id="@+id/end"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="확인"
            android:layout_marginRight="10dp"
            android:backgroundTint="#BDBDBD"
            android:textColor="#000000"/>
    </LinearLayout>

</LinearLayout>